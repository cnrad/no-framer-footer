{
    "manifest_version": 2,
    "name": "no-framer-footer",
    "version": "1.0",
    "icons": {
        "48": "icon.png"
    },
    "description": "Remove the 'free plan' footer in a Framer project",
    "content_scripts": [
        {
            "matches": ["*://framer.com/projects/*"],
            "js": ["script.js"]
        }
    ]
}